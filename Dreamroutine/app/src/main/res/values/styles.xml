<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="hello_">
        <item name="android:textSize">28sp</item>
        <item name="android:letterSpacing">0.05</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="not_a_member">
        <item name="android:textSize">15sp</item>
        <item name="android:letterSpacing">0.05</item>
        <item name="android:textColor">#E6757575</item>
    </style>

    <style name="register">
        <item name="android:textSize">15sp</item>
        <item name="android:letterSpacing">0.05</item>
        <item name="android:textColor">#FCD129</item>
    </style>
    
    <style name="Swipe_view">
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowTranslucentStatus">false</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    
    <style name="temp">
        <item name="android:statusBarColor">#00000000</item>
    </style>

    <style name="DropMenu" parent="Widget.MaterialComponents.TextInputLayout.FilledBox">
        <item name="android:textColorHint">#5A5A5A</item>
        <item name="android:textSize">16sp</item>
        <item name="colorControlNormal">#5A5A5A</item>
        <item name="colorControlActivated">@color/black</item>
    </style>

</resources>